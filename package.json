{
  "name": "@enoviah/s-js-api",
  "version": "0.0.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "setup": "npm i",
    "e2e": "echo [EnoviahCI] E2E Command not configured."
  },
  "dependencies": {
    "@enoviah/nest-core": "latest",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "jsonschema": "^1.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "husky": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "enoviah": {
    "configuration": {
      "lifecycle": {
        "macos": {
          "onInit": {
            "commands": []
          },
          "onArtifact": {
            "commands": []
          },
          "onCodeQuality": {
            "commands": []
          }
        },
        "linux": {
          "onInit": {
            "commands": []
          },
          "onArtifact": {
            "commands": []
          },
          "onCodeQuality": {
            "commands": []
          }
        }
      },
      "rootPath": ".",
      "type": "service",
      "name": "s-js-api",
      "group": "enoviah"
    },
    "binary": {
      "artifact": {
        "linux": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        },
        "macos": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        }
      }
    },
    "library": {
      "artifact": {
        "linux": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        },
        "macos": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        }
      }
    },
    "service": {
      "artifact": {
        "linux": {
          "coverage": false,
          "e2e": false,
          "lint": true,
          "mandatory": [
            {
              "path": "package.json",
              "name": "package.json"
            },
            {
              "path": "Dockerfile",
              "name": "Dockerfile"
            },
            {
              "path": "node_modules",
              "name": "node_modules"
            },
            {
              "path": "docker",
              "name": "docker"
            }
          ],
          "test": false
        },
        "macos": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        }
      },
      "deployment": {
        "production": {
          "network": "traefik",
          "labels": {
            "traefik.port": "80",
            "traefik.frontend.rule": "Host:js-api.enoviah.fr",
            "traefik.frontend.entryPoints": "http,https",
            "traefik.backend": "js-api-enoviah-fr"
          },
          "volumes": [
          ],
          "env": [
            "CLIENT_ID",
            "CLIENT_SECRET",
            "MONGO_DATABASE",
            "OAUTH2_API",
            "PORT",
            "MONGO_ADDRESS",
            "MONGO_HOST",
            "MONGO_PASSWORD",
            "MONGO_PORT",
            "MONGO_USER",
            "NODE_ENV"
          ]
        },
        "staging": {
          "network": "traefik",
          "labels": {
            "traefik.port": "80",
            "traefik.frontend.rule": "Host:js-js.enoviah.fr",
            "traefik.frontend.entryPoints": "http,https",
            "traefik.backend": "api-jeprecommande-fr"
          },
          "volumes": [
            {
              "pathInContainer": "/usr/src/api/public",
              "name": "s-cd-api",
              "driver": "local",
              "driverOpts": {
                "type": "nfs4",
                "o": "addr=37.187.0.142,rw",
                "device": ":/srv/nfs/portainer_docker_staging/s-cd-api"
              },
              "nfsPath": "/srv/nfs/portainer_docker_staging/s-cd-api"
            }
          ],
          "env": [
            "CLIENT_ID",
            "CLIENT_SECRET",
            "MONGO_DATABASE",
            "OAUTH2_API",
            "PORT",
            "MONGO_ADDRESS",
            "MONGO_HOST",
            "MONGO_PASSWORD",
            "MONGO_PORT",
            "MONGO_USER",
            "NODE_ENV",
            "URL",
            "UPLOAD_DIR",
            "UPLOAD_DIR_FOR_ITEM",
            "UPLOAD_LIMIT",
            "GMAPS_API_KEY",
            "APP_URL",
            "STRIPE_WEBHOOKS_KEY",
            "STRIPE_PRIVATE_KEY",
            "POSTMARK_API_KEY"
          ]
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "post-checkout": "eno-ci pc"
    }
  }
}
